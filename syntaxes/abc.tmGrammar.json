{
	"scopeName": "source.egg",
	"patterns": [{ "include": "#expression" }],
	"repository": {
	  "expression": {
		"patterns": [
			{ "include": "#commentTag" },
			{ "include": "#comment" },
			{ "include": "#texPath" },
			{ "include": "#keywords" },
			{ "include": "#kwcoll" },
			{ "include": "#objtype" },


			{ "include": "#transform" },
			{ "include": "#subtransform" },
			{ "include": "#scalar" },
			{ "include": "#scalarTypes" },
			{ "include": "#texture" },
			{ "include": "#vertexgroup" },
			{ "include": "#maingroup" },
			{ "include": "#subgroup" },
			{ "include": "#collectiongroup" },

			{ "include": "#subvertexgroup" },


			{ "include": "#attributeHeader" },
			{ "include": "#paren-expression1" },
			{ "include": "#paren-expression" }

		]
	  },
	  "texture": {
		"match": "Texture",
		"name": "entity.other.attribute-name.egg"
	  },
	  "scalar": {
		"match": "Scalar",
		"name": "variable.other.egg"
	  },
	  "scalarTypes": {
		"match": "alpha|fps|minfilter|magfilter",
		"name": "variable.other.egg"
	  },
	  "objtype": {
		"match": "ObjectType",
		"name": "markup.underline.egg"
	  },
	  "keywords": {
		"match": "rgb|rgba|luminance|structured|linear|mipmap|modulate|dual|binary|blend|blend_no_occlude|ms|clamp|repeat|ground|sphere|barrier|shground|floor|tube|axis|net|local",
		"name": "markup.italic.egg"
	  },
	  "kwcoll": {
		"match": "collisions|coll|collision",
		"name": "support.variable.egg"
	  },
	  "transform": {
		"match": "Transform",
		"name": "markup.bold.egg"
	  },
	  "subtransform": {
		"match": "Matrix4|Matrix3",
		"name": "markup.bold.egg"
	  },
	  "maingroup": {
		"match": "Group",
		"name": "entity.name.function.egg"
	  },
	  "subgroup": {
		"match": "VertexPool|Polygon|Model|BFace|DCS",
		"name": "variable.other.egg"
	  },
	  "collectiongroup": {
		"match": "Vertex",
		"name": "variable.other.egg"
	  },
	  "vertexgroup": {
		"match": "Normal|RGBA|UV|TRef|MRef|VertexRef|Ref|Translate",
		"name": "support.type.egg"
	  },
	  "subvertexgroup": {
		"match": "Tangent|Binormal",
		"name": "support.variable.egg"
	  },
	  "paren-expression": {
		"begin": "{",
		"end": "}",
		"beginCaptures": {
		  "4": { "name": "punctuation.paren.open.egg" }
		},
		"endCaptures": {
		  "4": { "name": "punctuation.paren.close.egg" }
		},
		"name": "entity.name.section.attribute.egg",
		"patterns": [{ "include": "#expression" }]
	  },
	  "paren-expression1": {
		"begin": "{",
		"end": "}",
		"beginCaptures": {
		  "2": { "name": "punctuation.paren.open.egg" }
		},
		"endCaptures": {
		  "2": { "name": "punctuation.paren.close.egg" }
		},
		"name": "entity.name.type.value.egg",
		"patterns": [{ "include": "paren-expression" }]
	  },
	  "attributeHeader": {
		"begin": "<",
		"end": ">",
		"beginCaptures": {
		  "1": { "name": "punctuation.paren.open.egg" }
		},
		"endCaptures": {
		  "1": { "name": "punctuation.paren.close.egg" }
		},
		"name": "entity.name.tag.attributeHeader.egg",
		"patterns": [{ "include": "#expression" }]
	  },
	  "texPath": {
		"begin": "\"",
		"end": "\"",
		"beginCaptures": {
		  "1": { "name": "punctuation.paren.open.egg" }
		},
		"endCaptures": {
		  "1": { "name": "punctuation.paren.close.egg" }
		},
		"name": "string.double.egg",
		"patterns": [{ "include": "#expression" }]
	  },
	  "commentTag": {
		"begin": "<Comment> {",
		"end": "}",
		"name": "comment.block.documentation.egg"
	  },
	  "comment": {
		"match": "//",
		"name": "comment.line.double-slash.egg"
	  }
	}
  }